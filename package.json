{
  "name": "empyrean_boilerplate",
  "version": "0.0.1",
  "description": "A super-slim boilerplate website including node server, typescript compilation and sass compilation",
  "main": "index.js",
  "author": "Craig Poole",
  "license": "MIT",
  "scripts": {
    "nodeserver": "http-server.cmd -p 8023",
    "tsc": "tsc -w",
    "sass": "node-sass ./style -o ./style --output-style compressed",
    "sass_w": "node-sass ./style -o ./style --output-style compressed --watch",
    "server": "concurrently \"npm run nodeserver\" \"npm run tsc\" \"npm run sass\" \"npm run sass_w\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AsciiSmoke/Empyrean.git"
  },
  "bugs": {
    "url": "https://github.com/AsciiSmoke/Empyrean/issues"
  },
  "homepage": "https://github.com/AsciiSmoke/Empyrean#readme",
  "dependencies": {
    "@types/react": "^16.0.18",
    "@types/react-dom": "^16.0.2",
    "awesome-typescript-loader": "^3.2.3",
    "concurrently": "^3.5.0",
    "express": "^4.16.2",
    "mongodb": "^2.2.33",
    "node-sass": "^4.5.3",
    "nodeserver": "^0.9.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sass": "^1.0.0-beta.2",
    "source-map-loader": "^0.2.3",
    "tsc": "^1.20150623.0",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "typescript": "^2.5.3"
  }
}
